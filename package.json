    "jest-environment-jsdom": "^29.2.2",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.18",
    "postcss-loader": "^7.0.1",
    "postcss-nested": "^6.0.0",
    "sass": "^1.87.0",
    "sass-loader": "^16.0.5",
    "storybook": "^7.6.17",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
