{
  "name": "fitcopilot-generator",
  "version": "1.0.0",
  "description": "AI-powered workout generation WordPress plugin",
  "main": "src/index.ts",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "validate-tokens": "node scripts/token-validator.js",
    "validate-tokens:verbose": "node scripts/token-validator.js --verbose",
    "validate-tokens:component": "node scripts/token-validator.js --component",
    "validate-tokens:feature": "node scripts/token-validator.js --feature",
    "test-colors": "node scripts/color-token-tester.js",
    "test-colors:verbose": "node scripts/color-token-tester.js --verbose",
    "test-colors:report": "node scripts/color-token-tester.js --report",
    "test-colors:component": "node scripts/color-token-tester.js --component=",
    "test-colors:feature": "node scripts/color-token-tester.js --feature=",
    "test-ts-colors": "node scripts/ts-color-validator.js",
    "test-ts-colors:verbose": "node scripts/ts-color-validator.js --verbose",
    "test-ts-colors:report": "node scripts/ts-color-validator.js --report",
    "test-ts-colors:component": "node scripts/ts-color-validator.js --component",
    "test-ts-colors:feature": "node scripts/ts-color-validator.js --feature",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "visual-regression": "node scripts/visual-regression.js",
    "visual-regression:ci": "CHROMATIC_PROJECT_TOKEN=${CHROMATIC_PROJECT_TOKEN} CI=true node scripts/visual-regression.js"
  },
  "author": "FitCopilot",
  "license": "GPL-2.0-or-later",
